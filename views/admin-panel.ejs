    function renderPagination(pagination) {
      const container = document.getElementById('programsPagination');
      
      if (pagination.totalPages <= 1) {
        container.innerHTML = '';
        return;
      }
      
      let html = '';
      
      // ì´ì „ í˜ì´ì§€
      if (pagination.hasPrevPage) {
        html += `<li class="page-item"><a class="page-link" href="#" onclick="loadPrograms(${pagination.currentPage - 1})">ì´ì „</a></li>`;
      }
      
      // í˜ì´ì§€ ë²ˆí˜¸ë“¤
      const startPage = Math.max(1, pagination.currentPage - 2);
      const endPage = Math.min(pagination.totalPages, pagination.currentPage + 2);
      
      for (let i = startPage; i <= endPage; i++) {
        html += `<li class="page-item ${i === pagination.currentPage ? 'active' : ''}">
                  <a class="page-link" href="#" onclick="loadPrograms(${i})">${i}</a>
                </li>`;
      }
      
      // ë‹¤ìŒ í˜ì´ì§€
      if (pagination.hasNextPage) {
        html += `<li class="page-item"><a class="page-link" href="#" onclick="loadPrograms(${pagination.currentPage + 1})">ë‹¤ìŒ</a></li>`;
      }
      
      container.innerHTML = html;
    }

